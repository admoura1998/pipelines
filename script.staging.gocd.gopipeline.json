{
	"format_version": 1,
	"group": "grupo",
	"name": "envia_arquivo_staging",
	"label_template": "${COUNT}",
	"parameters": [],
	"mingle": {},
	"tracking_tool": null,
	"environment_variables": [],
	"materials": [{
			"pipeline": "envia_arquivo",
			"stage": "gerar_artefato",
			"name": "arquivo.zip",
			"type": "dependency"
		}
	],
	"lock_behavior": "lockOnFailure",
	"stages": [{
		"name": "deploy",
		"fetch_materials": false,
		"never_cleanup_artifacts": false,
		"clean_working_directory": true,
		"approval": {
			"type": "manual"
		},
		"environment_variables": [],
		"jobs": [{
			"name": "deploy_staging",
			"run_instance_count": null,
			"environment_variables": [],
			"timeout": 0,
			"elastic_profile_id": null,
			"tabs": [],
			"resources": [
				"agent"
			],
			"artifacts": [],
			"properties": [],
			"tasks": [
                {
					"type": "fetch",
					"run_if": "passed",
					"pipeline": "envia_arquivo",
					"stage": "gerar_artefato",
					"job": "compactar_arquivo",
					"is_source_a_file": true,
					"source": "arquivo.zip",
					"destination": ""
				},                			
				{
					"type": "exec",
					"run_if": "passed",
					"on_cancel": null,
					"command": "unzip",
