{
	"format_version": 1,
	"group": "grupo",
	"name": "envia_arquivo_staging",
	"label_template": "${COUNT}",
	"parameters": [],
	"mingle": {},
	"tracking_tool": null,
	"environment_variables": [],
	"materials": [{
			"pipeline": "envia_arquivo",
			"stage": "gerar_artefato",
			"name": "arquivos.zip",
			"type": "dependency"
		}
	],
	"lock_behavior": "lockOnFailure",
	"stages": [{
		"name": "deploy",
		"fetch_materials": false,
		"never_cleanup_artifacts": false,
		"clean_working_directory": true,
		"approval": {
			"type": "manual"
		},
		"environment_variables": [],
		"jobs": [{
			"name": "deploy_staging",
			"run_instance_count": null,
			"environment_variables": [],
			"timeout": 0,
			"elastic_profile_id": null,
			"tabs": [],
			"resources": [
				"agent"
			],
			"artifacts": [],
			"properties": [],
			"tasks": [
                {
					"type": "fetch",
					"run_if": "passed",
					"pipeline": "envia_arquivo",
					"stage": "gerar_artefato",
					"job": "compactar_arquivo",
					"is_source_a_file": true,
					"source": "arquivos.zip",
					"destination": "arquivos"
				},                			
				{
					"type": "exec",
					"run_if": "passed",
					"on_cancel": null,
					"command": "/bin/bash",
					"arguments": ["-c", "unzip -u arquivos.zip"],
					"working_directory": "arquivos"
				},
				{
					"type": "exec",
					"run_if": "passed",
					"on_cancel": null,
					"command": "/bin/bash",
					"arguments": ["-c", "scp -i /home/go/.ssh/id_rsa -v * alvaro@172.19.80.197:/home/alvaro"],
					"working_directory": "arquivos"
				}
			]
		}]
	}]
}